{
  "globals": {
  },
  "parser": "babel-eslint",
  "extends": "airbnb/base",
  "env": {
    "es6": true,
    "node": true,
    "mocha": true
  },
  "ecmaFeatures": {
    "classes": true,
    "modules": true
  },
  "rules": {
    "class-methods-use-this": [
      0
    ],
    "max-len": [
      2,
      {
        "code": 150,
        "tabWidth": 4,
        "ignoreUrls": true
      }
    ],
    "no-underscore-dangle": [
      2,
      {
        "allow": [
          "__"
        ]
      }
    ],
    "comma-dangle": 0,
    "indent": [
      2,
      "tab"
    ],
    "no-tabs": 0,
    "space-before-function-paren": [
      "off"
    ],
    "import/no-unresolved": "on"
  }
}